{
  "name": "deepagents",
  "version": "1.0.2",
  "description": "General purpose 'deep agent' with sub-agent spawning, todo list capabilities, and mock file system",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Shelex/deepagents-ts.git"
  },
  "scripts": {
    "build": "tsc",
    "dev": "node --import tsx src/examples/research/agent.ts",
    "test": "jest"
  },
  "keywords": [
    "langchain",
    "langgraph",
    "agents",
    "llm",
    "ai",
    "typescript"
  ],
  "author": "Oleksandr Shevtsov",
  "license": "MIT",
  "dependencies": {
    "@langchain/anthropic": "^0.3.25",
    "@langchain/community": "^0.3.49",
    "@langchain/core": "^0.3.66",
    "@langchain/langgraph": "^0.4.2",
    "@lmstudio/sdk": "^1.4.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^12.0.1",
    "@types/node": "^22.17.0",
    "jest": "^29.7.0",
    "semantic-release": "^24.0.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3",
    "typescript": "^5.0.0"
  },
  "release": {
    "branches": [
      "master",
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git",
      "@semantic-release/github"
    ]
  }
}